/* pages/member/m-address/m-address.wxss */
page{background:#f5f5f5;}
.container{padding-top:20rpx;}
.list{width:100%;background:#f5f5f5;float:left;margin-bottom:100rpx;}
.address-list{width:100%;box-sizing: border-box;padding:0 20rpx;border-bottom:1px solid #f5f5f5;float:left;margin-bottom: 20rpx;background-color: #fff;}
.address-list .user{font-size:30rpx;color:#333;line-height:64rpx;}
.address-list .user text{float:left;}
.address-list .user .phone{float:right;color:#666;}
.address-list .receiving{font-size:30rpx;color:#333;line-height:64rpx;}
.address-list .operation{border-top:1px solid #f5f5f5;}
.address-list .operation label{line-height:80rpx;}
.address-list .operation label radio{float:left;line-height:72rpx;}
.address-list .operation label text{float:left;font-size:28rpx;color:#666}
.address-list .operation view{float:right; color:#666;line-height:80rpx;font-size:28rpx;margin-left:20rpx;}
.address-list .operation image{float:left;width:48rpx;margin:16rpx 10rpx;}
.bottom{position:fixed;height:100rpx;width:100%;bottom:0;left:0;}
.bottom navigator{width:100%;height:100rpx;text-align:center; line-height:100rpx; color:#fff;background:#ff4949;font-size:34rpx;}
.bottom navigator text{font-size:64rpx;vertical-align:top;height:100rpx;display:inline-block;line-height:94rpx;}








